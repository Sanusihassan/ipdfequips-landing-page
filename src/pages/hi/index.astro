---
const lang = "hi";
import { landing_page, tool, footer } from "../../content/content-hi";
import Header from "../../components/Header.astro";
import { OpenGraph } from "pdfequips-open-graph/OpenGraph";
import { Footer } from "pdfequips-footer/components/Footer";
import LandingPage from "../../components/LandingPage";
import SaveTokenScript from "../../components/SaveTokenScript.astro";
import "../../index.scss";
import Icons from "../../components/Icons.astro";
import { ConditionalAdsense } from "../../components/ConditionalAdsense";
const websiteSchema = {
  "@context": "http://schema.org",
  "@type": "WebSite",
  name: "PDFEquips",
  url: "https://www.pdfequips.com",
  description: `PDFEquips एक मुफ़्त, वेब-आधारित प्लेटफ़ॉर्म है जिसमें ऑनलाइन PDF फ़ाइलों को प्रबंधित और संपादित करने के लिए उपकरणों का संग्रह है। यह उपयोगकर्ताओं को कन्वर्ट करना, मर्ज करना, विभाजित करना और PDF पर हस्ताक्षर करना जैसी विभिन्न क्रियाएँ करने की अनुमति देता है, बिना डेस्कटॉप सॉफ़्टवेयर इंस्टॉल किए। यह सेवा पेशेवरों, छात्रों और व्यवसायों द्वारा तेज़ और कुशल दस्तावेज़ प्रसंस्करण के लिए उपयोग की जाती है, जिसमें सुरक्षा पर ध्यान दिया जाता है, जिसमें निर्धारित समय के बाद फ़ाइलों को स्वचालित रूप से हटाना शामिल है।`,
  sameAs: [
    "https://www.facebook.com/pdfequipsfb",
    "https://twitter.com/PDFEquips",
    "https://www.linkedin.com/company/pdfequips",
  ],
};
---

<html lang={lang} dir="ltr">
  <head>
    <meta charset="utf-8" />
    <!-- <link rel="icon" type="image/svg+xml" href="/images/icons/logo.svg" /> -->
    <meta name="generator" content={Astro.generator} />
    <title>PDFEquips | मुफ्त ऑनलाइन PDF उपकरण</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta
      name="description"
      content="PDFEquips पेश करता है मुफ्त ऑनलाइन टूल्स पेशेवरों, छात्रों और व्यक्तियों के लिए PDF फ़ाइलों को प्रबंधित, संपादित और ऑप्टिमाइज़ करने के लिए—महंगे सॉफ़्टवेयर की कोई आवश्यकता नहीं।"
    />
    <link rel="icon" href="/logo.png" />
    <link rel="apple-touch-icon" href="/apple-touch-icon.png" />
    <meta
      name="keywords"
      content="पीडीएफ उपकरण, पीडीएफ में कनवर्ट करें, जेपीजी से पीडीएफ, वर्ड से पीडीएफ, पावरपॉइंट से पीडीएफ, एक्सेल से पीडीएफ, एचटीएमएल से पीडीएफ, पीडीएफ से कनवर्ट करें, पीडीएफ से जेपीजी, पीडीएफ से वर्ड, पीडीएफ से पावरपॉइंट, पीडीएफ से एक्सेल, पीडीएफ से पीडीएफ/ए, पीडीएफ से टेक्स्ट, पीडीएफ को मर्ज करें, पीडीएफ को कंप्रेस करें।"
    />
    <OpenGraph
      ogDescription="PDFEquips - निःशुल्क पीडीएफ उपकरण | PDFEquips.com के साथ अपनी पीडीएफ आवश्यकताओं को सरल बनाएं"
      ogUrl="https://www.pdfequips.com/hi/"
      ogLocale="hi_IN"
      ogImage="https://www.pdfequips.com/pdfequips-hi.png"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" cross-origin="" />
    <link
      href="https://fonts.googleapis.com/css2?family=Raleway:ital,wght@0,100;0,300;0,400;0,500;0,600;0,700;0,800;1,100;1,200;1,300;1,400;1,500;1,600;1,700&display=swap"
      rel="stylesheet"
    />
    <script
      type="application/ld+json"
      set:html={JSON.stringify(websiteSchema)}
    />
    <meta http-equiv="Content-Language" content={lang} />
    <!-- <Alternatives /> -->
    <Icons />
    <SaveTokenScript />
    <meta
      name="ahrefs-site-verification"
      content="a532671b5b7f09bad30e7e65542ecf2f8f28a1ef5a25b2bec358924427540367"
    />
  </head>
  <body>
    <ConditionalAdsense client:load />
    <Header lang={lang} />
    <LandingPage
      landing_page={landing_page}
      tool={tool}
      lang={lang}
      client:load
    />
    <Footer
      lang={lang}
      includeTools={false}
      title="मुफ्त ऑनलाइन PDF उपकरण"
      client:load
    />
  </body>
</html>
