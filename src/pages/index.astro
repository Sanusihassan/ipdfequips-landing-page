---
import { landing_page, tool, footer } from "../content/content";
import Header from "../components/Header.astro";
import { OpenGraph } from "pdfequips-open-graph/OpenGraph";
import Footer from "../components/Footer.astro";
import LandingPage from "../components/LandingPage";
import "../index.scss";
import Analytics from "../components/Analytics.astro";
import Icons from "../components/Icons.astro";
// import Alternatives from "../components/Alternatives.astro";
const lang = "";
// const websiteSchema = {
// 	"@context": "http://schema.org",
// 	"@type": "WebSite",
// 	name: "PDFEquips",
// 	url: "https://www.pdfequips.com",
// 	description: `Enhance Your Productivity with our Comprehensive PDF Solution. ${allTitlesCombined}. Easy, Fast, and Secure Editing Online.`,
